{
  "name": "language-power",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "dev:all": "npm-run-all --parallel dev:server dev:client dev:build:server",
    "webpack": "webpack",
    "dev": "yarn webpack --env.mode development --watch",
    "prod": "yarn webpack -- --env.mode production",
    "dev:client": "yarn dev --env.presets client",
    "prod:client": "yarn prod -- --env.presets client --watch",
    "prod:analyze": "yarn prod -- --env.presets analyze",
    "prod:test": "yarn prod:analyze -- --env.presets test",
    "dev:build:server": "yarn dev -- --env.presets server --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "debugThis:": "yarn node --inspect --inspect-brk webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marusyaganza/language-power.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marusyaganza/language-power/issues"
  },
  "homepage": "https://github.com/marusyaganza/language-power#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "uuid": "^3.3.3",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "react-redux": "^7.1.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^2.1.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.3.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
