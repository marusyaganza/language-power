{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "**/*.@(jpg|jpeg|png|svg|webp|css|woff|js)",
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=31536000"
    } ]
    }]
  }
}
